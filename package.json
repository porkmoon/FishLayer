{
  "name": "fish-layer",
  "productName": "FishLayer",
  "version": "1.0.0",
  "type": "module",
  "description": "Your stealth workspace",
  "main": "electron/main.cjs",
  "author": "Ginger",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "vite build && electron-builder",
    "build:win": "vite build && electron-builder --win"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "electron-store": "^8.1.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^4.5.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.ginger.fishlayer",
    "productName": "FishLayer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
